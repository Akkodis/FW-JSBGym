            "args": ["--tele-file", "telemetry/test.csv",
                    "--scale"],
            "console": "integratedTerminal",
            "justMyCode": true,
        },
        {
            "name": "a.py",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}/jsbgym",
            "program": "a.py",
            "console": "integratedTerminal",
