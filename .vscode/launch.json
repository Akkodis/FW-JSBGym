        {
            "name": "ppo_eval",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}/jsbgym",
            "program": "ppo_eval.py",
            "args": ["--config", "config/ppo_caps.yaml",
                    "--env-id", "ACNoVaIntegErr-v0",
                    "--train-model", "models/train/ppo_caps_tuned1_integerr_2_88648_12-12_23:32:52.pt",
                    "--render-mode", "plot",
                    "--turb", "--wind", "--rand-targets", "--rand-atmo-mag"],
            "console": "integratedTerminal",
            "justMyCode": false,
            
        },
        {
            "name": "pid_eval",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}/jsbgym",
            "program": "pid_eval.py",
            "args": [
                    "--render-mode", "plot_scale",
                    "--rand-targets"],
                    // "--turb", "--wind"],
            "console": "integratedTerminal",
            "justMyCode": false,
        },
       {
            "name": "ppo_train",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}/jsbgym",
            "program": "ppo_train.py",
            "args": ["--exp-name", "debug_exp", "--num-envs", "2",
                "--total-timesteps", "10000", "--track", 
                "--env-id", "ACNoVa-v0",
                "--turb", "--wind", "--rand-atmo-mag", "--rand-targets"
                // "--config", "config/ppo_caps_no_va.yaml",
                // "--config", "config/ppo_caps.yaml",
                ],
            "console": "integratedTerminal",
            "justMyCode": false,
        },
        {
